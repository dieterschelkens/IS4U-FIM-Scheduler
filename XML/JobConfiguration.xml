<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  <processing-directives>
	<overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>
  <schedule>
	<job>
	  <name>Run</name>
	  <job-type>IS4U.RunConfiguration.RunJob, IS4U.RunConfiguration</job-type>
	</job>
	<trigger>
	  <cron>
		<name>syncPortal</name>
		<description>Start the scheduler with run configuration 'FIM'.</description>
		<job-name>Run</job-name>
		<job-data-map>
		  <entry>
			<key>RunConfigName</key>
			<value>FIM</value>
		  </entry>
		</job-data-map>
		<misfire-instruction>DoNothing</misfire-instruction>
		<cron-expression>0/10 0/1 * * * ?</cron-expression>
	  </cron>
	</trigger>
  </schedule>
</job-scheduling-data>