<?xml version="1.0" encoding="utf-8"?>
<RunConfig xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="RunSchedulingData.xsd">
	<ClearRunHistory>true</ClearRunHistory>
	<KeepHistory Days="5" />
	<DelayInParallelSequence Seconds="2" />
	<DelayInLinearSequence Seconds="2" />
	<OnDemandSchedule>Delta</OnDemandSchedule>

	<!-- Run profiles -->
	<RunConfiguration Name="Full" Profile="Full import full sync">
		<Step Type="LinearSequence">FIM</Step>
		<Step Type="ManagementAgent" Action="Delta import delta sync">FIM MA</Step>
	</RunConfiguration>
	<RunConfiguration Name="Delta" Profile="Delta import delta sync">
		<Step Type="LinearSequence">FIM</Step>
		<Step Type="ManagementAgent">FIM MA</Step>
	</RunConfiguration>

	<!-- Sequences -->
	<Sequence Name="FIM">
		<Step Type="ManagementAgent">FIM MA</Step>
		<Step Type="ManagementAgent" Action="Export">AD MA</Step>
		<Step Type="Delay" Seconds="10" />
		<Step Type="ManagementAgent">AD MA</Step>
		<Step Type="ManagementAgent" Action="Export">FIM MA</Step>
	</Sequence>
</RunConfig>